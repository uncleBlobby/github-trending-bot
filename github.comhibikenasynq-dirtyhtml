        <span class="pl-s1">log</span>.<span class="pl-en">Fatalf</span>(<span class="pl-s">"could not enqueue task: %v"</span>, <span class="pl-s1">err</span>)
    }
    <span class="pl-s1">log</span>.<span class="pl-en">Printf</span>(<span class="pl-s">"enqueued task: id=%s queue=%s"</span>, <span class="pl-s1">info</span>.<span class="pl-c1">ID</span>, <span class="pl-s1">info</span>.<span class="pl-c1">Queue</span>)


    <span class="pl-c">// ------------------------------------------------------------</span>
    <span class="pl-c">// Example 2: Schedule task to be processed in the future.</span>
