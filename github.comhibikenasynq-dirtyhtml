<span class="pl-k">func</span> <span class="pl-en">main</span>() {
    <span class="pl-s1">client</span> <span class="pl-c1">:=</span> <span class="pl-s1">asynq</span>.<span class="pl-en">NewClient</span>(asynq.<span class="pl-smi">RedisClientOpt</span>{<span class="pl-c1">Addr</span>: <span class="pl-s1">redisAddr</span>})
