          <a role="menuitem" class="filter-item SelectMenu-item py-1 text-emphasized" style="padding-left: 12px;" data-action="click:readme-toc#blur" data-targets="readme-toc.entries" data-hydro-click="{&quot;event_type&quot;:&quot;repository_toc_menu.click&quot;,&quot;payload&quot;:{&quot;target&quot;:&quot;entry&quot;,&quot;repository_id&quot;:29028775,&quot;originating_url&quot;:&quot;https://github.com/facebook/react-native&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="7c35614422f06799ea7e5fb362a86e758c3a92e017c9210c761cbc47d2a28462" href="#------react-native--">
  
